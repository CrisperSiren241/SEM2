<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="mainbackground" TargetType="StackPanel">
        <Setter Property="Background" Value="#C9EBF9"/>
    </Style>

    <Style x:Key="scrollbackground" TargetType="ScrollViewer">
        <Setter Property="Background" Value="#C9EBF9"/>
    </Style>

    <Style x:Key="darkbutton" TargetType="Button">
        <Setter Property="Background" Value="#C9EBF9"/>
        <Setter Property="Foreground" Value="Black"/>
    </Style>

    <Style x:Key="lightbutton" TargetType="Button">
        <Setter Property="Background" Value="#062343"/>
        <Setter Property="Foreground" Value="white"/>
    </Style>

    <Style x:Key="SearchColor" TargetType="TextBlock">
        <Setter Property="Foreground" Value="black"/>
    </Style>

</ResourceDictionary>