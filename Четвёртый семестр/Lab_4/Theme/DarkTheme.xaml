<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="mainbackground" TargetType="Grid">
        <Setter Property="Background" Value="#251D59"/>
    </Style>

    <Style x:Key="secondarybackground" TargetType="Grid">
        <Setter Property="Background" Value="#010626"/>
    </Style>

    <Style x:Key="Products" TargetType="TextBlock">
        <Setter Property="Foreground" Value="White"/>
    </Style>

    

</ResourceDictionary>